<configuration >
  <preference name="nlpTools" value="yes" />
  <preference name="dbUser" value="sa" />
  <preference name="loadFresh" value="false" />
  
  <!-- MultiWord annotator for use - WordNet or DBPedia, for WordNet provide wnmw, for DBPedia give dbpmw -->
  <preference name="multiWordAnnotatorType" value="dbpmw" />
  <!-- DBPedia dictionary files directory -->
  <preference name="dbpediaSrcDir" value="$SIGMA_HOME/dbpedia" />

